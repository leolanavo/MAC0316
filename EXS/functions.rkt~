#lang racket

(define curry
  (lambda (f)
    (lambda (x)
      (lambda (y) (f x y)))))

(define mapcar (f l)
  (if (null? l) '()
    (cons (f (car l)) (mapcar f (cdr l)))))

(define mapc (curry mapcar))

(define combine (f1 f2 zero)
  (lambda (l)
    (if (null? l) zero
      (f2 (f1 (car l)) ((combine f1 f2 zero) (cdr l))))))

(define id
  (lambda (x) x))

(define cdr* (l)
  (mapc cdr l))

(define mkfnpair
